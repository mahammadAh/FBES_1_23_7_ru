<UserControl x:Class="WPFNavigation.Views.ThirdScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFNavigation.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="360">
    <StackPanel>
        <Label Height="40" Grid.Column="1" FontSize="15" Content="{Binding LabelText}" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>

    </StackPanel>
</UserControl>
